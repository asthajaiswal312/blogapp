<%- include("partials/header") -%>
    <h1>Home</h1>
    <p>
        <%= homeStartingContent %>
    </p>
    <% posts.forEach(post=> { %>
        <div class="blogpost">
            <h2>
                <%= post.title %>
            </h2>
            <p style="display: inline;">
                <%= post.content.substring(0, 100) + "..." %>
            </p>
            <a href="/posts/<%= post.title %> ">Read more</a>
        </div>
        <% }); %>

            <%- include("partials/footer") -%>